<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <!--Text Styles-->
    <style name="Widget" />

    <style name="Widget.ShakoMako" />

    <style name="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/text_size_body</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">normal</item>
        <item name="android:letterSpacing" tools:targetApi="lollipop">0.03125</item>
        <item name="android:textColorHint">@color/colorAccent</item>
        <item name="fontFamily">@font/poppins_regular</item>

    </style>

    <style name="Widget.ShakoMako.Text.Black" parent="Widget.ShakoMako.Text">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Green" parent="Widget.ShakoMako.Text">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="Widget.ShakoMako.Text.Black.Bold" parent="Widget.ShakoMako.Text.Black">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/poppins_semibold</item>
=    </style>

    <style name="Widget.ShakoMako.Text.Large" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_18sdp</item>

        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="font">@font/poppins_regular</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <!--        <item name="android:fontFamily">@font/lato_bold</item>-->
    </style>

    <style name="Widget.ShakoMako.Text.Large.Bold" parent="Widget.ShakoMako.Text.Large">
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/poppins_semibold</item>
    </style>

    <style name="Widget.ShakoMako.Button.Default" parent="Widget.ShakoMako.Text">
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="Widget.ShakoMako.Text.Heading" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:gravity">center</item>
        <item name="fontFamily">@font/poppins_semibold</item>

        <!--        <item name="android:fontFamily">@font/lato_regular</item>-->
    </style>

    <style name="Widget.ShakoMako.Text.Title" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:elevation" tools:ignore="NewApi">0dp</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="Widget.ShakoMako.Text.Subtitle.Black" parent="Widget.ShakoMako.Text.Subtitle">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Subtitle" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="fontFamily">@font/poppins_regular</item>
    </style>

    <style name="Widget.ShakoMako.Text.Body" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/text_size_body</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="Widget.ShakoMako.Text.Body.White" parent="Widget.ShakoMako.Text.Body">
        <item name="android:textColor">@color/colorAccent</item>
    </style>

    <style name="Widget.ShakoMako.Text.Body.Black" parent="Widget.ShakoMako.Text.Body">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Menu" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.ShakoMako.Text.Content" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">start</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.ShakoMako.Text.Toolbar" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="Widget.ShakoMako.Text.Small" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_10sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.ShakoMako.Text.Small.Black" parent="Widget.ShakoMako.Text.Black">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Small.White" parent="Widget.ShakoMako.Text">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Widget.ShakoMako.Text.Small.Green" parent="Widget.ShakoMako.Text.Small">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="Widget.ShakoMako.Text.Small.Red" parent="Widget.ShakoMako.Text.Black">
        <item name="android:textColor">@android:color/holo_red_dark</item>
    </style>

    <style name="Widget.ShakoMako.Text.Smaller" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_6sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Tiny" parent="Widget.ShakoMako.Text">
        <item name="android:textSize">@dimen/_8sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="Widget.ShakoMako.Text.Tiny.Black" parent="Widget.ShakoMako.Text.Tiny">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="Widget.ShakoMako.Text.Large.Grey" parent="Widget.ShakoMako.Text.Large">
        <item name="android:textColor">@android:color/darker_gray</item>
    </style>

    <style name="Widget.ShakoMako.Text.Button" parent="Widget.ShakoMako.Text">
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:background">@drawable/button_bg_primary</item>
        <item name="android:layout_margin">@dimen/_20sdp</item>
        <item name="android:padding">@dimen/_10sdp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>

    </style>


    <style name="Widget.ShakoMako.Text.Button.Border" parent="Widget.ShakoMako.Text.Button">
        <item name="android:background">@drawable/button_border_primary</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textStyle">normal</item>

    </style>


    <style name="Widget.ShakoMako.Text.Title.Gray" parent="Widget.ShakoMako.Text.Title">
        <item name="android:textColor">@android:color/darker_gray</item>

    </style>

